            <file targetdir="$INSTALL_PATH/marauroa_server_manager/modules" src="dist/marauroa_server_manager/marauroa_server_manager/modules/simple-marauroa-client-lib.jar"/>
            <file targetdir="$INSTALL_PATH/marauroa_server_manager/modules" src="dist/marauroa_server_manager/marauroa_server_manager/modules/simple-server-lib.jar"/>
        </pack>
        <pack name="jWrestling" preselected="no" installGroups="marauroa_server_manager" required="no" hidden="false">
