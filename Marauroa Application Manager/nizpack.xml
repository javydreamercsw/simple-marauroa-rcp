            <file targetdir="$INSTALL_PATH/marauroa_server_manager/update_tracking" src="dist/marauroa_server_manager/marauroa_server_manager/update_tracking/marauroa-lib.xml"/>
            <file targetdir="$INSTALL_PATH/marauroa_server_manager/update_tracking" src="dist/marauroa_server_manager/marauroa_server_manager/update_tracking/simple-marauroa-application-api.xml"/>
            <file targetdir="$INSTALL_PATH/marauroa_server_manager/update_tracking" src="dist/marauroa_server_manager/marauroa_server_manager/update_tracking/simple-marauroa-application-core.xml"/>
