            <file targetdir="$INSTALL_PATH/marauroa_server_manager/update_tracking" src="dist/marauroa_server_manager/marauroa_server_manager/update_tracking/simple-marauroa-application-gui.xml"/>
            <file targetdir="$INSTALL_PATH/marauroa_server_manager/update_tracking" src="dist/marauroa_server_manager/marauroa_server_manager/update_tracking/eclipselink.xml"/>
            <file targetdir="$INSTALL_PATH/marauroa_server_manager/update_tracking" src="dist/marauroa_server_manager/marauroa_server_manager/update_tracking/simple-marauroa-application-monitor.xml"/>
            <file targetdir="$INSTALL_PATH/marauroa_server_manager/update_tracking" src="dist/marauroa_server_manager/marauroa_server_manager/update_tracking/simple-marauroa-client-lib.xml"/>
            <file targetdir="$INSTALL_PATH/marauroa_server_manager/update_tracking" src="dist/marauroa_server_manager/marauroa_server_manager/update_tracking/simple-server-lib.xml"/>
            <file targetdir="$INSTALL_PATH/marauroa_server_manager/modules/" src="dist/marauroa_server_manager/marauroa_server_manager/modules/ext"/>
            <file targetdir="$INSTALL_PATH/marauroa_server_manager/modules/" src="dist/marauroa_server_manager/marauroa_server_manager/modules/locale"/>
            <file targetdir="$INSTALL_PATH/marauroa_server_manager/modules" src="dist/marauroa_server_manager/marauroa_server_manager/modules/marauroa-lib.jar"/>
            <file targetdir="$INSTALL_PATH/marauroa_server_manager/modules" src="dist/marauroa_server_manager/marauroa_server_manager/modules/simple-marauroa-application-api.jar"/>
            <file targetdir="$INSTALL_PATH/marauroa_server_manager/modules" src="dist/marauroa_server_manager/marauroa_server_manager/modules/simple-marauroa-application-core.jar"/>
            <file targetdir="$INSTALL_PATH/marauroa_server_manager/modules" src="dist/marauroa_server_manager/marauroa_server_manager/modules/simple-marauroa-application-core-db.jar"/>
            <file targetdir="$INSTALL_PATH/marauroa_server_manager/modules" src="dist/marauroa_server_manager/marauroa_server_manager/modules/simple-marauroa-application-gui.jar"/>
            <file targetdir="$INSTALL_PATH/marauroa_server_manager/modules" src="dist/marauroa_server_manager/marauroa_server_manager/modules/eclipselink.jar"/>
